<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<!-- frameStrata Parameters:  BACKGROUND, LOW, MEDIUM, HIGH, DIALOG, FULLSCREEN, FULLSCREEN_DIALOG, TOOLTIP  -->
	<!-- Layer level Parameters:  BACKGROUND, BORDER, ARTWORK, OVERLAY, HIGHLIGHT -->

	<!-- SciFi_AnchorFrame -->
	<Frame name="SciFi_AnchorFrame" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT"><Offset><AbsDimension x="0" y="-1"/></Offset></Anchor>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"><Offset><AbsDimension x="0" y="-1"/></Offset></Anchor>
		</Anchors>
	</Frame>
	
	<!-- SpartanUI -->
	<Frame name="SciFi_SpartanUI" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativeTo="SciFi_AnchorFrame" relativePoint="TOPLEFT" />
			<Anchor point="TOPRIGHT" relativeTo="SciFi_AnchorFrame" relativePoint="TOPRIGHT">
				<Offset><AbsDimension x="0" y="153"/></Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parent_Base1" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\base-center">
					<Anchors><Anchor point="BOTTOM" relativePoint="BOTTOM"/></Anchors>
				</Texture>
				<Texture name="$parent_Base2" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\base-left1">
					<Anchors><Anchor point="RIGHT" relativeTo="$parent_Base1" relativePoint="LEFT"/></Anchors>
				</Texture>
				<Texture name="$parent_Base3" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\base-left1" tile="true">
					<Anchors><Anchor point="RIGHT" relativeTo="$parent_Base2" relativePoint="LEFT"/></Anchors>
				</Texture>
				<Texture name="$parent_Base4" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\base-left1" >
					<Anchors><Anchor point="LEFT" relativeTo="$parent_Base1" relativePoint="RIGHT"/></Anchors>
				</Texture>
				<Texture name="$parent_Base5" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\base-left1" tile="true">
					<Anchors><Anchor point="LEFT" relativeTo="$parent_Base4" relativePoint="RIGHT"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_StatusBars_TooltipTemplate -->
	<Frame name="SciFi_StatusBars_TooltipTemplate" frameStrata="TOOLTIP" virtual="true">
		<Size><AbsDimension x="374" y="70"/></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-tooltip">
					<Anchors><Anchor point="CENTER"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parentHeader" inherits="SciFi_FontOutline12">
					<Size><AbsDimension x="390" y="16" /></Size>
					<Anchors><Anchor point="TOP"/></Anchors>
				</FontString>
				<FontString name="$parentText" inherits="SciFi_FontOutline10">
					<Size><AbsDimension x="378" y="46" /></Size>
					<Anchors><Anchor point="TOP" relativeTo="$parentHeader" relativePoint="BOTTOM">
						<Offset><AbsDimension x="0" y="-2"/></Offset>
					</Anchor></Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_StatusBars_XPTemplate -->
	<Frame name="SciFi_StatusBars_XPTemplate" frameStrata="BACKGROUND" enableMouse="true" virtual="true">
		<Size><AbsDimension x="400" y="32" /></Size>
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parentPlate" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-plate-exp">
					<Size><AbsDimension x="400" y="32" /></Size>
					<Anchors><Anchor point="RIGHT"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentFill" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow">
					<Size><AbsDimension x="400" y="32" /></Size>
					<Anchors><Anchor point="RIGHT" /></Anchors>
				</Texture>
				<Texture name="$parentLead" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow">
					<Size><AbsDimension x=".1" y="32" /></Size>
					<Anchors><Anchor point="RIGHT" relativeTo="$parentFill" relativePoint="LEFT"/></Anchors>
					<TexCoords left="1" right="0" top="0" bottom="1" />
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentFillGlow" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentFill" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentFill" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentLeadGlow" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentLead" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentLead" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
					<TexCoords left="1" right="0" top="0" bottom="1" />
				</Texture>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parentBase" setAllPoints="true">
					<Color r="0" g="0" b="0" a=".95"/>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_StatusBars_RepTemplate -->
	<Frame name="SciFi_StatusBars_RepTemplate" frameStrata="BACKGROUND" enableMouse="true" virtual="true">
		<Size><AbsDimension x="400" y="32" /></Size>
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parentPlate" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-plate-rep">
					<Size><AbsDimension x="400" y="32" /></Size>
					<Anchors><Anchor point="LEFT"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentFill" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow">
					<Size><AbsDimension x="400" y="32" /></Size>
					<Anchors><Anchor point="LEFT"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentFillGlow" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\status-glow" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentFill" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentFill" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parentBase" setAllPoints="true">
					<Color r="0" g="0" b="0" a="0.95"/>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_PopTemplate -->
	<Frame name="SciFi_PopTemplate" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="400" y="32" /></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\bar-backdrop0">
					<Size><AbsDimension x="512" y="64" /></Size>
					<Anchors><Anchor point="BOTTOM" relativePoint="BOTTOM"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_BarTemplate1 -->
	<Frame name="SciFi_BarTemplate1" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="400" y="32" /></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\bar-backdrop1">
					<Size><AbsDimension x="512" y="64" /></Size>
					<Anchors><Anchor point="CENTER" relativePoint="CENTER"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_BarTemplate3 -->
	<Frame name="SciFi_BarTemplate3" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="135" y="100" /></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\bar-backdrop3">
					<Size><AbsDimension x="256" y="128" /></Size>
					<Anchors><Anchor point="CENTER" relativePoint="CENTER"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_ActionBarsTemplate -->
	<Frame name="SciFi_ActionBarsTemplate" parent="SciFi_SpartanUI" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="1002" y="139"/></Size>
		<Frames>
			<Frame name="SciFi_Bar1" inherits="SciFi_BarTemplate1"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-100" y="70" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Bar2" inherits="SciFi_BarTemplate1"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-100" y="31" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Bar3" inherits="SciFi_BarTemplate1"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="100" y="70" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Bar4" inherits="SciFi_BarTemplate1"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="100" y="31" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Bar5" inherits="SciFi_BarTemplate3"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-502" y="5" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Bar6" inherits="SciFi_BarTemplate3"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="502" y="5" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Popup1" inherits="SciFi_PopTemplate"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-100" y="105" /></Offset></Anchor></Anchors></Frame>
			<Frame name="SciFi_Popup2" inherits="SciFi_PopTemplate"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="100" y="105" /></Offset></Anchor></Anchors></Frame>
		</Frames>
	</Frame>
	
	<!-- SciFi_Popup1MaskTemplate -->
	<Frame name="SciFi_Popup1MaskTemplate" parent="SciFi_SpartanUI" frameStrata="MEDIUM" virtual="true">
		<Size><AbsDimension x="400" y="34"/></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\bar-popup1">
					<Size><AbsDimension x="512" y="64" /></Size>
					<Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- SciFi_Popup2MaskTemplate -->
	<Frame name="SciFi_Popup2MaskTemplate" parent="SciFi_SpartanUI" frameStrata="MEDIUM" virtual="true">
		<Size><AbsDimension x="400" y="34"/></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Artwork\Themes\SciFi\Images\bar-popup2">
					<Size><AbsDimension x="512" y="64" /></Size>
					<Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

