<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<!-- frameStrata Parameters:  BACKGROUND, LOW, MEDIUM, HIGH, DIALOG, FULLSCREEN, FULLSCREEN_DIALOG, TOOLTIP  -->
	<!-- Layer level Parameters:  BACKGROUND, BORDER, ARTWORK, OVERLAY, HIGHLIGHT -->
	<Script file="Style-Setup.lua" />

	<!-- SpartanUI -->
	<Frame name="Fel_SpartanUI" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOM" relativeTo="UIParent" relativePoint="BOTTOM" />
		</Anchors>
		<Size><AbsDimension y="153" /></Size>
	</Frame>
	
	<!-- Fel_StatusBars_TooltipTemplate -->
	<Frame name="Fel_StatusBars_TooltipTemplate" frameStrata="TOOLTIP" virtual="true">
		<Size><AbsDimension x="374" y="70"/></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-tooltip">
					<Anchors><Anchor point="CENTER"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parentHeader" inherits="Fel_FontOutline12">
					<Size><AbsDimension x="390" y="16" /></Size>
					<Anchors><Anchor point="TOP"/></Anchors>
				</FontString>
				<FontString name="$parentText" inherits="Fel_FontOutline10">
					<Size><AbsDimension x="378" y="46" /></Size>
					<Anchors><Anchor point="TOP" relativeTo="$parentHeader" relativePoint="BOTTOM">
						<Offset><AbsDimension x="0" y="-2"/></Offset>
					</Anchor></Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- Fel_StatusBars_XPTemplate -->
	<Frame name="Fel_StatusBars_XPTemplate" frameStrata="BACKGROUND" enableMouse="true" virtual="true">
		<Size><AbsDimension x="400" y="10" /></Size>
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parentPlate" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-plate-rep">
					<Size><AbsDimension x="400" y="10" /></Size>
					<Anchors><Anchor point="RIGHT"/></Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentFill" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-glow">
					<Size><AbsDimension x="400" y="10" /></Size>
					<Anchors><Anchor point="RIGHT" /></Anchors>
				</Texture>
				<Texture name="$parentLead" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-glow">
					<Size><AbsDimension x=".1" y="10" /></Size>
					<Anchors><Anchor point="RIGHT" relativeTo="$parentFill" relativePoint="LEFT"/></Anchors>
					<TexCoords left="1" right="0" top="0" bottom="1" />
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentFillGlow" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-glow" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentFill" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentFill" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentLeadGlow" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-glow" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentLead" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentLead" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
					<TexCoords left="1" right="0" top="0" bottom="1" />
				</Texture>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parentPlate" file="Interface\AddOns\SpartanUI_Style_Fel\Images\status-plate-rep">
					<Size><AbsDimension x="400" y="10" /></Size>
					<Anchors><Anchor point="LEFT"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- Fel_BarTemplate1 -->
	<Frame name="Fel_BarTemplate1" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="400" y="32" /></Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI_Style_Transparent\Images\bar-backdrop1">
					<Size><AbsDimension x="512" y="64" /></Size>
					<Anchors><Anchor point="CENTER" relativePoint="CENTER"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	
	<!-- Fel_ActionBarsTemplate -->
	<Frame name="Fel_ActionBarsTemplate" parent="Fel_SpartanUI" frameStrata="BACKGROUND" virtual="true">
		<Size><AbsDimension x="1002" y="139"/></Size>
		<Frames>
			<Frame name="Fel_Bar1" inherits="Fel_BarTemplate1"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-100" y="50" /></Offset></Anchor></Anchors></Frame>
			<Frame name="Fel_Bar2" inherits="Fel_BarTemplate1"><Anchors><Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM"><Offset><AbsDimension x="-100" y="5" /></Offset></Anchor></Anchors></Frame>
			<Frame name="Fel_Bar3" inherits="Fel_BarTemplate1"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="100" y="50" /></Offset></Anchor></Anchors></Frame>
			<Frame name="Fel_Bar4" inherits="Fel_BarTemplate1"><Anchors><Anchor point="BOTTOMLEFT" relativePoint="BOTTOM"><Offset><AbsDimension x="100" y="5" /></Offset></Anchor></Anchors></Frame>
		</Frames>
	</Frame>
	
	<Script file="Module-Artwork.lua" />
	<Script file="Module-UnitFrames.lua" />
	<Script file="Style.lua" />
</Ui>